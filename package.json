  "files": [
    "dist/**/*"
  ],
  "peerDependencies": {
    "formik": "^1.0.0",
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/api": "^5.2.8",
    "@storybook/components": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@types/node": "^12.12.14",
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "@types/yup": "^0.26.26",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "formik": "^1.5.8",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "typescript": "^3.7.2",
    "yup": "^0.27.0"

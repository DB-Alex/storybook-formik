{
  "name": "storybook-formik",
  "version": "1.1.0",
  "description": "A storybook decorator that allows you to use components in your stories that rely on Formik context.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc",
    "build:watch": "npm run build -- --watch",
    "postversion": "git push && git push --tags && npm publish",
    "storybook": "start-storybook -p 6006"
